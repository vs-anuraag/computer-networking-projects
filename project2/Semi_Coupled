set terminal pdf
set output "allPlots.pdf"
set title "DataRate vs Time\n\n"

set terminal postscript eps enhanced color solid font 'Times-Bold,15'
set output "DataRate.eps"
set xlabel "Time (s)" offset 0,-1
set ylabel "DataRate (Mbps)" offset 0,0 
set grid
set lmargin 10.0
set rmargin 5.0
set key bmargin center horizontal Left reverse noenhanced autotitles columnhead nobox

plot "-"  title "Flow 1" with linespoints, "-"  title "Flow 2" with linespoints, "-"  title "Flow 5" with linespoints
